{
  "name": "lofty-npm-template",
  "version": "0.0.6",
  "description": "React npm template",
  "author": "Alex Marmon",
  "license": "MIT",
  "repository": "https://github.com/alexmarmon/react-mobx-template.git",
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "node server.js",
    "lint": "eslint --ext .js --ext .jsx src/; exit 0",
    "lint-fix": "eslint --fix --ext .js --ext .jsx src/; exit 0",
    "build": "webpack --config webpack.dist.js --progress --colors --optimize-minimize"
  },
  "keywords": [
    "react",
    "mobx",
    "hot reload 3",
    "template",
    "starter-kit",
    "react router 4",
    "webpack 3"
  ],
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "globals": {
      "document": true,
      "fetch": true,
      "console": true,
      "it": true,
      "describe": true,
      "window": true
    },
    "rules": {
      "no-console": 0,
      "window": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "radix": 0,
      "forbid-prop-types": 0,
      "react/forbid-prop-types": 0,
      "no-nested-ternary": 0,
      "max-len": 0,
      "react/require-default-props": 0,
      "react/jsx-wrap-multilines": 0,
      "import/first": 0
    }
  },
  "babel": {
    "presets": [
      "react",
      "stage-1",
      "es2015"
    ],
    "plugins": [
      "transform-class-properties",
      "react-hot-loader/babel"
    ]
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.15.2",
    "file-loader": "^0.11.2",
    "lofty-pretty-print-plugin": "^1.0.0",
    "node-sass": "^4.5.2",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-hot-loader": "next",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-server": "^2.5.0",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "extract-text-webpack-plugin": "^3.0.0"
  }
}
